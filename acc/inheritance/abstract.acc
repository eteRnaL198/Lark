abstract aspect AbstractAspect {
  constructor(
    type,
    var,
    display,
    msg
  )

  abstract void method()

  void method2() {
    printf(this.msg);
  }

  before(): execution(this.type baz()) {
    this.var = 10;
    this.method();
    if(1) {
      this.method2();
    }
    this.display(this.msg);
  }
  
}