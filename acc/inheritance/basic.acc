aspect BasicAspect {
  void func() {
    printf("basic aspect method\n");
  }
  
  after(): execution(int foo()) {
    this.func();
  }
}

aspect Hoge {
  after(): execution(int foo()) {
    printf("hoge aspect method\n");
    if(1) {
      this.func();
      if(1) {
        this.func();
      }
    }
  }
}