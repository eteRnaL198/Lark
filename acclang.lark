?start: aspect
aspect: advice"():" pointcut"(" ")" "{" body+ "}"

advice: before | after | around
before: "before"
after: "after"
around: "around"
body: /.+/

pointcut: joinpoint+
joinpoint: call | execution | get | set
call: "call"
execution: "execution"
get: "get"
set: "set"


